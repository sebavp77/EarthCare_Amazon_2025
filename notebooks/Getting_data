        accumulator[\"histograms\"][k] += result[\"histograms\"][k]\n","\n","        for k in accumulator[\"joint_histograms\"]:\n","            accumulator[\"joint_histograms\"][k] += result[\"joint_histograms\"][k]\n","\n","        accumulator[\"latlon_counts\"] += result[\"latlon_counts\"]\n"],"metadata":{"id":"sWKuLCrPqdVN","executionInfo":{"status":"ok","timestamp":1767794696809,"user_tz":300,"elapsed":5858,"user":{"displayName":"Sebastian Vergara Palacio","userId":"13447388179824386780"}}},"execution_count":78,"outputs":[]},{"cell_type":"code","source":[],"metadata":{"id":"mBJb8IRpZ1Ni","executionInfo":{"status":"ok","timestamp":1767796316731,"user_tz":300,"elapsed":73,"user":{"displayName":"Sebastian Vergara Palacio","userId":"13447388179824386780"}}},"execution_count":90,"outputs":[]},{"cell_type":"code","source":["cloud_phase_dict = {\n","    1: \"Water\",\n","    2: \"Ice\",\n","    3: \"Supercooled\",\n","    4: \"Overlap\",\n","    -127: \"Not determined\"\n","}\n","phase_codes = np.array([ -127,1, 2, 3, 4])\n","\n","\n","plot_histogram(\n","    counts = accumulator[\"histograms\"][\"cloud_phase\"],\n","    category_dict = cloud_phase_dict,\n","    category_codes = phase_codes,\n","    figsize=(7, 4),\n","    ylabel=\"Frequency\",\n","    xlabel=\"Cloud phase\",\n","    title=None,\n","    ax=None,\n","    show=True,\n",");"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":401},"id":"r7jf5UkF2Dss","executionInfo":{"status":"ok","timestamp":1767795934247,"user_tz":300,"elapsed":240,"user":{"displayName":"Sebastian Vergara Palacio","userId":"13447388179824386780"}},"outputId":"18502dde-4d07-4ea4-eb09-c93cb81e1416"},"execution_count":89,"outputs":[{"output_type":"display_data","data":{"text/plain":["<Figure size 700x400 with 1 Axes>"],"image/png":"\n"},"metadata":{}}]},{"cell_type":"code","source":["x_edges = np.concatenate(([-127.5], np.arange(0.5, 10.5, 1)))\n","y_edges = np.concatenate(([-127.5], np.arange(0.5, 5.5, 1)))\n","\n","H = a